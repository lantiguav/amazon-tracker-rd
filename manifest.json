{
  "name": "Amazon Tracker RD",
  "description": "Extension to track packages with international shipping to Dominican Republic",
  "version": "1.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://www.amazon.com/*"],
      "js": ["content.js"]
    }
  ]
}
